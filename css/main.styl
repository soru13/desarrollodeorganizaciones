			margin-bottom  20px
			width  216px
			vertical-align  top
			h4
				margin-top 0px
			p
				text-align left
			.LandingSkills-image
				// margin-bottom  5px
	.LandingSummary-list
		column-count  3
		flex-flow  wrap
		justify-content  space-between
		overflow  hidden
		padding  0
		.LandingSummary-item
			list-style  none
			text-align  left
			padding  5px
			box-sizing  border-box
			position  relative
			& before
				content '.'
				position  absolute
				left  0
				top  0
				font-weight  bold
